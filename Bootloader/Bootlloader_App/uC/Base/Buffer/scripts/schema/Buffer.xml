<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<paramdef>
  <name>Buffer</name>
  <paramobject type="paramcontainer">
    <name>Config</name>
    <description>Buffer Config Container</description>
    <lowermultiplicity>1</lowermultiplicity>
    <uppermultiplicity>1</uppermultiplicity>
    <paramobject type="listcontainer">
      <name>RingBuffers</name>
      <description>List of All Ring Buffers</description>
      <lowermultiplicity>1</lowermultiplicity>
      <uppermultiplicity>1</uppermultiplicity>
      <paramobject type="paramcontainer">
        <name>RingBuffer</name>
        <description>Ring Buffer Definition</description>
        <lowermultiplicity>1</lowermultiplicity>
        <uppermultiplicity>1</uppermultiplicity>
        <paramobject type="dataobject" data="integer">
          <name>Length</name>
          <description>Data Length of the Ring Buffer</description>
          <lowermultiplicity>1</lowermultiplicity>
          <uppermultiplicity>1</uppermultiplicity>
          <default>1</default>
          <min>1</min>
          <max>255</max>
        </paramobject>
        <paramobject type="dataobject" data="boolean">
          <name>Enabled</name>
          <description>Enables or Disables a Ring Buffer</description>
          <lowermultiplicity>1</lowermultiplicity>
          <uppermultiplicity>1</uppermultiplicity>
          <default>true</default>
        </paramobject>
      </paramobject>
    </paramobject>
  </paramobject>
</paramdef>